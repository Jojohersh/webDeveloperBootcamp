<%- include("partials/header") %>
<h1>Blogs:</h1>

<% blogs.forEach((blog)=>{ %>
  <div class="">
    <h2><%= blog.title%></h2>
    <img src="<%=blog.image%>" alt="">
    <span><%=blog.created%></span>
    <p><%=blog.body.substring(0,75)%>...</p>
    <a class="ui inverted blue button" href="/blogs/<%=blog._id%>">Read More...</a>
  </div>
<% }) %>
<%- include("partials/footer") %>
