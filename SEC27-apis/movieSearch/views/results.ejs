<h1>Results Page</h1>
<ul>
  <% if (data.Search) {%>
    <% data.Search.forEach((movie) => { %>
      <li>
        <strong>
          <%=movie.Title%>
        </strong>
        - <%=movie.Year%>
      </li>
    <% }) %>
  <%} else {%>
    <h2>No Results Found</h2>
  <%}%>
</ul>

<a href="/">Search Again!</a>
